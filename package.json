{
  "name": "phaser-kit",
  "version": "1.0.0",
  "description": "A starter project for Phaser by leveraging the power of Typescript, Parcel bundler and Cordova.",
  "main": "index.js",
  "repository": "git@github.com:vickylance/phaser-kit.git",
  "author": "Vignesh S",
  "license": "MIT",
  "private": false,
  "scripts": {
