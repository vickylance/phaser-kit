    "parcel-bundler": "^1.10.3",
    "prettier": "1.14.3",
    "pretty-quick": "^1.8.0",
    "sass": "^1.14.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
